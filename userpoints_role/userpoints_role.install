// $Id: userpoints_role.install,v 1.2 2008-12-18 02:08:34 kbahey Exp $

function userpoints_role_uninstall() {
  db_delete('variable')
    ->condition(db_or()
      ->condition('name', 'userpoints_role%', 'LIKE')
      ->condition('name', 'userpoints_email%', 'LIKE')
    )
    ->execute();
}
